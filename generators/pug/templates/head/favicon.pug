mixin favicon(domain)
  link(rel="icon" href=domain+"/public/favicon/favicon-16.png" sizes="16x16" type="image/png")
  link(rel="icon" href=domain+"/public/favicon/favicon-32.png" sizes="32x32" type="image/png")
  link(rel="icon" href=domain+"/public/favicon/favicon-48.png" sizes="48x48" type="image/png")
  link(rel="icon" href=domain+"/public/favicon/favicon-62.png" sizes="62x62" type="image/png")
  link(rel="icon" href=domain+"/public/favicon/favicon-192.png" sizes="192x192" type="image/png")
